<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>prodest-requisicao-renovacao-ear</groupId>
	<artifactId>prodest-requisicao-renovacao-ear</artifactId>
	<version>0.0.1</version>
	<packaging>ear</packaging>
	<name>prodest-requisicao-renovacao-ear</name>
	
	<parent>
		<groupId>prodest-parent</groupId>
		<artifactId>prodest-parent</artifactId>
		<version>0.0.1-SNAPSHOT</version>
		<relativePath>../prodest-parent</relativePath>
	</parent>
	
	<build>
		<defaultGoal>package</defaultGoal>
		<finalName>prodest-requisicao-ear</finalName>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>2.0.2</version>
				<executions>
					<execution>
						<id>default-compile</id>
						<phase>compile</phase>
						<goals>
							<goal>compile</goal>
						</goals>
						<configuration>
							<source>1.6</source>
							<target>1.6</target>
							<excludes>
								<exclude>**/.svn</exclude>
							</excludes>
						</configuration>
					</execution>
					<execution>
						<id>default-testCompile</id>
						<phase>test-compile</phase>
						<goals>
							<goal>testCompile</goal>
						</goals>
						<configuration>
							<source>1.6</source>
							<target>1.6</target>
							<excludes>
								<exclude>**/.svn</exclude>
							</excludes>
						</configuration>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<artifactId>maven-ear-plugin</artifactId>
				<version>2.4.2</version>				
				<configuration>					
					<defaultLibBundleDir>lib</defaultLibBundleDir>
					<includeLibInApplicationXml>false</includeLibInApplicationXml>
					<filtering>false</filtering>
					<version>5</version>
					<modules>
						<ejbModule>
							<groupId>org.jboss.seam</groupId>
							<artifactId>jboss-seam</artifactId>
							<bundleDir>/</bundleDir>
							<bundleFileName>jboss-seam.jar</bundleFileName>
						</ejbModule>
						<ejbModule>
							<groupId>prodest-requisicao-ejb</groupId>
							<artifactId>prodest-requisicao-ejb</artifactId>
							<bundleFileName>prodest-requisicao-ejb.jar</bundleFileName>
							<bundleDir>/</bundleDir>
						</ejbModule>
						<webModule>
							<groupId>prodest-renovacao</groupId>
							<artifactId>prodest-renovacao</artifactId>
							<bundleDir>/</bundleDir>
							<contextRoot>/prodest-renovacao</contextRoot>
							<bundleFileName>prodest-renovacao.war</bundleFileName>
						</webModule>
						<webModule>
							<groupId>prodest-assinatura-digital-web</groupId>
							<artifactId>prodest-assinatura-digital-web</artifactId>
							<bundleDir>/</bundleDir>
							<contextRoot>/assinatura-digital</contextRoot>
							<bundleFileName>assinatura-digital.war</bundleFileName>
						</webModule>
						<webModule>
							<groupId>prodest-requisicao</groupId>
							<artifactId>prodest-requisicao</artifactId>							
							<bundleDir>/</bundleDir>
							<contextRoot>/prodest-requisicao</contextRoot>
							<bundleFileName>prodest-requisicao.war</bundleFileName>
						</webModule>

					</modules>
				</configuration>
			</plugin>
			
		
		<plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>jboss-maven-plugin</artifactId>
        <version>1.5.0</version>
        <configuration>
          <hostName>localhost</hostName>
          <port>8080</port>
          <fileNames>
          	<fileName>${project.build.directory}/${project.build.finalName}.ear</fileName>
          </fileNames>
          <serverName>default</serverName>
        </configuration>
      </plugin>
	</plugins>
	</build>


